{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install",
  "framework": "nextjs",
  "rootDirectory": "frontend",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://coopwise.onrender.com",
    "NEXT_PUBLIC_WS_URL": "wss://coopwise.onrender.com",
    "NEXT_PUBLIC_USE_MOCK_DATA": "false",
    "NEXT_PUBLIC_BASE_URL": "https://coopwise-seven.vercel.app",
    "NEXT_PUBLIC_APP_URL": "https://coopwise-seven.vercel.app",
    "NEXT_PUBLIC_ENABLE_MOCK_FALLBACK": "false"
  }
}
